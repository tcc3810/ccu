    .scope S_003EA2F8;
T_0 ;
    %wait E_003E9420;
    %load/v 8, v005C1DA8_0, 4;
    %cmpi/u 8, 8, 4;
    %jmp/1 T_0.0, 6;
    %cmpi/u 8, 9, 4;
    %jmp/1 T_0.1, 6;
    %load/v 8, v005C1DA8_0, 4;
    %mov 12, 0, 2;
    %load/v 14, v003E31B8_0, 4;
    %mov 18, 0, 2;
    %add 8, 14, 6;
    %load/v 14, v003E3210_0, 4;
    %mov 18, 0, 2;
    %add 8, 14, 6;
    %load/v 14, v003E3268_0, 4;
    %mov 18, 0, 2;
    %add 8, 14, 6;
    %set/v v005AE5B0_0, 8, 4;
    %set/v v005BD920_0, 12, 1;
    %set/v v005BD978_0, 13, 1;
    %jmp T_0.3;
T_0.0 ;
    %movi 8, 3, 4;
    %ix/load 0, 4, 0;
    %assign/v0 v005AE5B0_0, 0, 8;
    %jmp T_0.3;
T_0.1 ;
    %movi 8, 3, 4;
    %ix/load 0, 4, 0;
    %assign/v0 v005AE5B0_0, 0, 8;
    %jmp T_0.3;
T_0.3 ;
    %load/v 8, v005BD978_0, 1;
    %load/v 9, v005BD920_0, 1;
    %or 8, 9, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v005BD8C8_0, 0, 8;
    %jmp T_0;
