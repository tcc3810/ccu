T_4.6 ;
    %vpi_func 2 47 "$random", 40, 32;
    %mov 8, 40, 32;
    %movi 40, 16, 32;
    %mod 8, 40, 32;
    %ix/load 0, 4, 0;
    %assign/v0 v005AF2F0_0, 0, 8;
