    %vpi_call 2 60 "$display", "correct answer: ov = %d, sum = %d", v005AE768_0, v005AF298_0;
    %vpi_call 2 61 "$display", "\012";
T_4.8 ;
T_4.1 ;
    %jmp T_4;
