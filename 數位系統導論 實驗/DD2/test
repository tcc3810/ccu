    %set/v v005AF298_0, 8, 4;
    %set/v v005BC290_0, 12, 1;
    %set/v v005AF3A0_0, 13, 1;
    %load/v 8, v005AF3A0_0, 1;
    %load/v 9, v005BC290_0, 1;
    %or 8, 9, 1;
    %set/v v005AE768_0, 8, 1;
